    "summary": "Summary",
    "sections": [
        {
            "activityTitle": "Details:",
            "facts": [
                { "name": "ğŸ“¦ Event type:", "value": "{{ event_type }}" },
                { "name": "ğŸ“„ Artefact name:", "value": "{{ artifact_name }}" },
                { "name": "ğŸ’¾ Repo key:", "value": "{{ artifact_repo_key }}" },
                { "name": "ğŸ“‚ Path:", "value": "{{ artifactory_path }}/{{ artifact_name }}" },
                { "name": "ğŸ“ Size:", "value": "{{ artifact_size }}" },
                { "name": "ğŸ”’ sha256:", "value": "{{ artifact_sha256 }}" },
                { "name": "â° Timestamp:", "value": "{{ timestamp }}" }
            ]
        }
    ],
    "potentialAction": [
        {
            "@type": "OpenUri",
            "name": "Open File",
            "targets": [
                { "os": "default", "uri": "{{ artifactory_url_base }}/{{ artifactory_path }}/{{ artifact_name }}" }
